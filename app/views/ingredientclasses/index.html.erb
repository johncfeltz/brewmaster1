<h1>Listing ingredientclasses</h1>

<table>
  <tr>
    <th>Ingredclass</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @ingredientclasses.each do |ingredientclass| %>
  <tr>
    <td><%= ingredientclass.ingredclass %></td>
    <td><%= link_to 'Show', ingredientclass %></td>
    <td><%= link_to 'Edit', edit_ingredientclass_path(ingredientclass) %></td>
    <td><%= link_to 'Destroy', ingredientclass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Ingredientclass', new_ingredientclass_path %>
